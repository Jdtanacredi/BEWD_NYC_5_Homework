<% @products.each do |product| %>
    <tr>
      <td><%= product.image_url %></td>
      <td><%= product.title %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <%= link_to 'Show', product %>
      <%= link_to 'Edit', edit_product_path(@product) %>
      <%= link_to 'Destroy', product_path(@product) %>
    </tr>
<% end %>